ch\n            </button>\n          </form>\n        </div>\n\n        <div className='button-container music-btns-list'>\n          {musicList.filter((musicItem) => !activeCategory || musicItem.tags.includes(activeCategory)).map((musicItem) => (<MusicBtn\n            onClick={() => playSong(musicItem.id)}\n            key={musicItem.id}\n            face={musicItem.face}\n            name={musicItem.name}/>))}\n        </div>\n\n        <div className='dock-background'>\n          <div className='dock-buttons'>\n            <div className='dock-text'>üéµ {activeMusic\n                ?.name ?? 'music'}</div>\n            {isPlaying\n              ? (\n                <div className='dock-button toggle-play' onClick={stopSong}>\n                  ‚è∏Ô∏è\n                </div>\n              )\n              : (\n                <div\n                  className='dock-button toggle-play'\n                  onClick={() => startSong(activeMusic.song)}>\n                  ‚ñ∂Ô∏è\n                </div>\n              )}\n            {/* <div className=\"dock-button toggle-volume\">üîä</div> */}\n          </div>\n        </div>\n\n        {/* <Dock /> */}\n      </div>\n    </div>\n  )\n}\n\nexport default MusicPlayer\n","/Users/yingxi/Desktop/IT_Project_022/client/src/navbar.js",["63","64"],[],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport './navbar.css'\n\nfunction Navbar () {\n  return (\n    <div className='navbar'>\n      <div className='nav-links'>\n        <Link to='/' className='nav-link'>üéµ Home</Link>\n        <a href='#' className='nav-link'>‚öô Setting</a>\n        <a href='#' className='nav-link'>üë§ Contact us</a>\n      </div>\n    </div>\n  )\n}\n\nexport default Navbar\n","/Users/yingxi/Desktop/IT_Project_022/client/src/homepage.js",[],[],"/Users/yingxi/Desktop/IT_Project_022/client/src/dock.js",[],[],"/Users/yingxi/Desktop/IT_Project_022/client/src/components/NavList.js",[],[],"/Users/yingxi/Desktop/IT_Project_022/cl